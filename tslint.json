{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "member-access": [
      "no-public"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "import-spacing": false,
    "member-ordering": false,
    "max-line-length": [
      true,
      150
    ],
    "semicolon": [
      true,
      "always"
    ],
    "indent": [
      true,
      "spaces",
      4
    ],
    "no-console": false
  },
  "rules": {
    "member-access": [
      true,
      "no-public"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "no-var-requires": true,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "import-spacing": false,
    "member-ordering": false,
    "interface-name": false,
    "max-line-length": [
      true,
      150
    ],
    "semicolon": [
      true,
      "always"
    ],
    "indent": [
      true,
      "spaces",
      4
    ],
    "no-console": false,
    "typedef": [
      false,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "property-declaration",
      "member-variable-declaration",
      "array-destructuring",
      "object-destructuring",
      "arrow-parameter",
      "variable-declaration"
    ]
  }
}
